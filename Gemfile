# A sample Gemfile
source "https://rubygems.org"

gem 'nokogiri'
gem 'faraday'
gem 'faraday_middleware'
gem 'concurrent-ruby', require: 'concurrent'
gem 'addressable'

group :test do
  gem 'rspec'
  gem 'webmock'
  gem 'sinatra'
  gem 'pry'
  gem 'pry-byebug'
  gem 'haml'
  gem 'simplecov', :require => false
end
